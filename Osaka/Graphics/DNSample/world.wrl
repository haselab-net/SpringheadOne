#VRML V2.0 utf8

DirectionalLight {
	direction 0 -1 0
	intensity 0.5
	ambientIntensity 0
}

Viewpoint{
	position 0 3 10
}

DEF FFrame Transform {
	translation 0 0 0
	children [
		#field
		Inline {url "field.wrl"}
	]
}

DEF PFrame Transform {
	translation 2 0 0
	children [
		#penguin
		Inline {url "penguin.wrl"}
	]
}

