#VRML V2.0 utf8

DEF PBFrame Transform {
	translation 0 1.4 0
	children [
		DEF PBody Transform {
			translation 0 0 0
			children [
				Shape {
					appearance Appearance {
						material Material {}
					}
					geometry Sphere {
						radius 1
					}
				}
			]
		}

		DEF PFFrame Transform {
			translation 0 -1.2 0
			children [
				DEF PFoot Transform {
					translation 0 0 0
					children [
						Shape {
							appearance Appearance {
								material Material {}
							}
							geometry Sphere {
								radius 0.2
							}
						}
					]
				}
			]
		}
	]
}
