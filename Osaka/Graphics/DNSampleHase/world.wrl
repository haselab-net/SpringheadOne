#VRML V2.0 utf8

DirectionalLight {
	direction 0 -1 0
	intensity 0.5
	ambientIntensity 0
}

Viewpoint{
	position 0 10 0
	orientation 0 -1 0 0
}

DEF FFrame Transform {
	translation 0 0 0
	children [
		#field
		Inline {url "field.wrl"}
	]
}

DEF PFrame Transform {
	translation 2 0 0
	children [
		#penguin
		Inline {url "real_pen.wrl"}
	]
}

DEF BPFrame Transform {
	translation -1 1 1
	children [
		#bigpuck
		Inline {url "big_puck.wrl"}
	]
}

DEF SPFrame Transform {
	translation 1 1 1
	children [
		#smallpuck
		Inline {url "small_puck.wrl"}
	]
}

